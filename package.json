{
  "name": "chrome-gemini-cli",
  "description": "CLI for autonomous browser automation using Gemini AI and Chrome DevTools MCP.",
  "version": "1.0.0",
  "type": "module",
  "bin": {
    "chrome-gemini": "src/index.ts"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "start": "tsx src/index.ts",
    "lint": "eslint src schemas",
    "lint:fix": "eslint src schemas --fix",
    "format": "prettier --write \"src/**/*.ts\" \"schemas/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"schemas/**/*.ts\"",
    "prepare": "husky"
  },
  "keywords": [
    "gemini",
    "chrome-devtools",
    "mcp",
    "browser-automation",
    "ai-agent",
    "web-scraping",
    "cli"
  ],
  "author": "Bronson Avila",
  "license": "ISC",
  "dependencies": {
    "@google/genai": "1.34.0",
    "@modelcontextprotocol/sdk": "1.20.1",
    "chrome-devtools-mcp": "0.9.0",
    "dotenv": "17.2.3",
    "wrap-ansi": "9.0.2"
  },
  "devDependencies": {
    "@types/node": "24.9.1",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "eslint": "9.38.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-perfectionist": "4.15.1",
    "husky": "9.1.7",
    "lint-staged": "16.2.6",
    "prettier": "3.4.2",
    "tsx": "4.20.6",
    "typescript": "5.9.3"
  },
  "lint-staged": {
    "{src,schemas}/**/*.ts": [
      "pnpm lint:fix",
      "pnpm format"
    ]
  }
}
